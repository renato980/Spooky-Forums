<main class="login-main">
		<h1 class="login-h1">Log In</h1>
		<div class="login-container">
				<form action="/read-a-db-record">
						<label for="username">Username</label>
						<input class="login-form" type="text" id="username" name="username" placeholder="Username">
						<label for="password">Password</label>
						<input class="login-form" type="password" id="password" name="password" placeholder="Password">
						<input class="login-form" type="submit" value="Log In" id="submit">
				</form>
		</div>
</main>
